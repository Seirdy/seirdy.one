{
	"extends": [
		"web-recommended"
	],
	"connector": {
		"name": "puppeteer",
		"options": {
			"headless": true,
			"browser": "Chromium"
		}
	},
	"hints": {
		"axe/other": "error",
		"https-only": "error",
		"doctype": "error",
		"performance-budget": "error",
		"compat-api/css": "error",
		"manifest-is-valid": "error",
		"manifest-file-extension": "error",
		"compat-api/html": [
			"error",
			{
				"ignore": [
					"time",
					"picture",
					"meta[name=color-scheme]"
				]
			}
		],
		"http-compression": [
			"warning",
			{
				"html": {
					"brotli": false
				},
				"resource": {
					"brotli": false
				}
			}
		]
	}
}

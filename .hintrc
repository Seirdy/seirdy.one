{
	"connector": {
		"name": "puppeteer",
		"options": {
			"browser": "Chromium",
			"headless": true
		}
	},
	"extends": [
		"web-recommended"
	],
	"formatters": [
		"codeframe"
	],
	"hints": {
		"apple-touch-icons": "off",
		"axe/other": "error",
		"compat-api/css": [
			"error",
			{
				"ignore": [
					"filter"
				]
			}
		],
		"compat-api/html": [
			"error",
			{
				"ignore": [
					"time",
					"picture",
					"meta[name=color-scheme]"
				]
			}
		],
		"doctype": "error",
		"http-compression": "error",
		"https-only": "error",
		"manifest-file-extension": "error",
		"manifest-is-valid": "error",
		"performance-budget": "error",
		"ssllabs": "off"
	}
}
